{
  "name": "ts-node-startkit",
  "version": "1.5.0",
  "description": "Lets you update GitHub checks with arbitrary information from the CLI, useful for CI processes.",
  "main": "src/index.ts",
  "scripts": {
    "start": "gh-checks",
    "build": "webpack --mode production",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --watchAll=false --coverage",
    "lint": "eslint .",
    "lint:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|md)\" && yarn lint --fix",
    "lint:ci": "yarn lint -o reports/eslint/issues.json -f json",
    "lint-staged": "lint-staged"
  },
  "author": {
    "name": "Mateu Aguil√≥ Bosch",
    "email": "mateu@mateuaguilo.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@formatjs/cli": "^3.1.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.0",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {}
}
